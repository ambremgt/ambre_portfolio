

<div class="home-content align-items-center d-flex justify-content-center">

      <%= image_tag ('centre.png'), class: 'img-home' %>

  <div class="navtop">
    <div class="d-flex justify-content-between">
      <div class="am20">AM20.</div>
      <div class="navtop-circle"></div>
    </div>
  </div>

  <div class="line-container">
    <div class="lines lines-black line1"></div>
    <div class="lines lines-black line5"></div>
    <div class="lines-horizontal lines-black line-h1"></div>
    <div class="lines-horizontal lines-black line-h2"></div>
  </div>

  <div class="social-container">
    <ul class="list-social-icons">
      <li><%= link_to "", 'https://github.com/ambremgt', target: '_blank', class: "fab fa-github fa-lg social-icon"%></li>
      <li><%= link_to "", 'https://www.linkedin.com/in/ambre-margerit-33441ba8/', target: '_blank', class: "fab fa-linkedin-in fa-lg social-icon"%></li>
      <li><%= link_to "", 'https://dribbble.com/Ambremgt', target: '_blank', class: "fab fa-dribbble fa-lg social-icon"%></li>
